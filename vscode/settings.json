{
  "editor.colorDecorators": false,
  "editor.detectIndentation": false,
  "editor.fontSize": 13,
  "editor.minimap.side": "left",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.stickyScroll.defaultModel": "indentationModel",
  "editor.tabSize": 2,
  "keyboard.dispatch": "keyCode",
  "window.titleBarStyle": "custom",
  "workbench.activityBar.location": "top",
  "workbench.sideBar.location": "right",
  "git.defaultBranchName": "main",
  "workbench.colorTheme": "Brio Terminal Yellow-Dark",
  "workbench.colorCustomizations": {
    "editorIndentGuide.background1": "#0000",
    "scrollbarSlider.background": "#fb09",
    "tab.border": "#0000",
    "tree.indentGuidesStroke": "#0000"
  },
  "codesnap.backgroundColor": "transparent",
  "codesnap.boxShadow": "#0009 0 .25em .75em",
  "codesnap.containerPadding": "1em",
  "codesnap.realLineNumbers": true,
  "codesnap.roundedCorners": true,
  "codesnap.showWindowControls": false,
  "codesnap.showWindowTitle": true,
  "github.copilot.chat.localeOverride": "es",
  "github.copilot.enable": {
    "markdown": true
  },
  "todo-tree.general.tags": [
    " edit ",
    " info ",
    " todo ",
    " warn "
  ],
  "todo-tree.highlights.defaultHighlight": {
    "borderRadius": "1em",
    "gutterIcon": true,
    "type": "tag"
  },
  "todo-tree.highlights.customHighlight": {
    " edit ":{
      "background": "#0f9",
      "foreground": "#000",
      "icon": "pencil",
      "iconColour": "#0f9"
    },
    " info ": {
      "background": "#09f",
      "foreground": "#000",
      "icon": "info",
      "iconColour": "#09f"
    },
    " todo ": {
      "background": "#fff",
      "foreground": "#000",
      "icon": "checklist",
      "iconColour": "#fff"
    },
    " warn ": {
      "background": "#fb0",
      "foreground": "#000",
      "icon": "alert",
      "iconColour": "#fb0"
    }
  }
}